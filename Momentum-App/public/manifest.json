{
  "manifest_version": 2,

  "name": "Fake Momentum App",
  "description": "This extension changes the default new tab to our app",
  "version": "1.0",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage",
    "geolocation"
  ]
}
